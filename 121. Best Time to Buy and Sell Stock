# O(n)
class Solution(object):
    def maxProfit(self, prices):
        """
        :type prices: List[int]
        :rtype: int
        """
        min_price = float('inf') # Set min_price to infinity
        max_profit = 0
        for price in prices:
            max_profit = max(price - min_price, max_profit)     
            if price < min_price:
                min_price = price
        return max_profit
        
