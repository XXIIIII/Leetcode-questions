# Approach 1 O(n)
class Solution(object):
    def moveZeroes(self, nums):
        """
        :type nums: List[int]
        :rtype: None Do not return anything, modify nums in-place instead.
        """
        count = 0
        for num in nums:
            if num != 0:
                nums[count] = num
                count += 1
        for i in range(count, len(nums)): # Replace zeroes at the end
            nums[i] = 0
            
        return nums

# Approach 2: Two pointers method O(n)
class Solution(object):
    def moveZeroes(self, nums):
        """
        :type nums: List[int]
        :rtype: None Do not return anything, modify nums in-place instead.
        """
        count = 0 # record index to place the next non-zero number
        for i in range(len(nums)):
            if nums[i]!=0:
                nums[i], nums[count] = nums[count], nums[i]
                count += 1
            
        return nums
        
        
