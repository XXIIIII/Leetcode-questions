// my code 
// time complexity O(n^2)
int* twoSum(int* nums, int numsSize, int target, int* returnSize){
    int *output = (int *)malloc(2*sizeof(int));
    int remain = 0;
    *returnSize = 2;
    for(int i = 0; i < numsSize; i++){
        remain = target - nums[i];
        for(int j = i+1; j < numsSize; j++){
            if(nums[j] == remain){
                output[0] = i;
                output[1] = j;
                return output;
            }
        }
    }
    return NULL;
}
