# Sliding Window
class Solution(object):
    def checkInclusion(self, s1, s2):
        """
        :type s1: str
        :type s2: str
        :rtype: bool
        """
        if len(s1) > len(s2):
            return False
            
        char_s1 = {}
        char_s2 = {}

        for char in s1:
            char_s1[char] = char_s1.get(char, 0) + 1
            
        for i in range(len(s2)):
            if i >= len(s1):
                if s2[i-len(s1)] in char_s1:
                    char_s2[s2[i-len(s1)]] -= 1
                    
            if s2[i] in char_s1:
                char_s2[s2[i]] = char_s2.get(s2[i], 0) + 1
                
            if char_s1 == char_s2:
                return True
        return False
